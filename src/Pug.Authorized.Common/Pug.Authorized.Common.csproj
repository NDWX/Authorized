<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>0.3</PackageVersion>
		<Title>Pug.Authorized.Common</Title>
		<PackageLicenseExpression>Apache-2.0+</PackageLicenseExpression>
		<License>Apache2.0</License>
		<RepositoryUrl>https://github.com/NDWX/Authorized.git</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageTags>Pug.Authorized Authorization Framework Common Types</PackageTags>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Company>Pug Computing</Company>
		<AssemblyVersion>0.3</AssemblyVersion>
		<FileVersion>0.3</FileVersion>
		<RootNamespace>Pug.Authorized</RootNamespace>
		<PackageReleaseNotes>Upgraded Pug.Application.Security to v2.0.0</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="Pug.Application.Data" Version="1.1.12" />
	  <PackageReference Include="Pug.Application.Security" Version="2.0.0" />
	  <PackageReference Include="Pug.Effable" Version="1.0.8" />
	  <PackageReference Include="SonarAnalyzer.CSharp" Version="8.31.0.39249">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

</Project>
